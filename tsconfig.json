{
    "compilerOptions": {
        "target": "es2017",
        "outDir": "dist",
        "jsx": "react",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "experimentalDecorators": true,
        "removeComments": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true
    }
}
